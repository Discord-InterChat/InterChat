    "editor.codeActionsOnSave": {
        "source.fixAll.eslint": true
      },
      "eslint.validate": ["javascript"],
      "appService.zipIgnorePattern": [
        "node_modules{,/**}",
        "build{,/**}",
        ".vscode{,/**}",
        ".git",
        "__tests__",
        "*.md",
        ".env.example"
      ]
}