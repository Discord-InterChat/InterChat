generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

type MessageDataChannelAndMessageIds {
  channelId String
  messageId String
}

type MessageDataReference {
  channelId String
  guildId   String
  messageId String
}

type SetupChannel {
  id   String
  name String
}

type SetupDate {
  full      DateTime @db.Date
  timestamp Int
}

type SetupGuild {
  id   String
  name String
}

type SetupWebhook {
  id    String
  token String
  url   String
}

type UserDataUser {
  id  String
  tag String
}

type UserDataWarnings {
  automated   Boolean
  moderatorId String?
  number      Int
  reason      String
  timestamp   Int?
}

model blacklistedServers {
  id         String @id @default(auto()) @map("_id") @db.ObjectId
  reason     String
  serverId   String
  serverName String
}

model blacklistedUsers {
  id       String  @id @default(auto()) @map("_id") @db.ObjectId
  notified Boolean
  reason   String
  userId   String
  username String
}

model connectedList {
  id          String @id @default(auto()) @map("_id") @db.ObjectId
  channelId   String
  channelName String
  serverId    String
  serverName  String
}

model levels {
  id          String   @id @default(auto()) @map("_id") @db.ObjectId
  v           Int      @map("__v")
  guildID     String
  lastUpdated DateTime @db.Date
  level       Int
  userID      String
  xp          Int
}

model messageData {
  id                   String                            @id @default(auto()) @map("_id") @db.ObjectId
  authorId             String
  channelAndMessageIds MessageDataChannelAndMessageIds[]
  expired              Boolean
  reference            MessageDataReference?
  serverId             String
  timestamp            Float
}

model restrictedWords {
  id    String   @id @default(auto()) @map("_id") @db.ObjectId
  name  String
  words String[]
}

model setup {
  id         String        @id @default(auto()) @map("_id") @db.ObjectId
  channel    SetupChannel
  compact    Boolean
  date       SetupDate
  guild      SetupGuild
  profFilter Boolean
  webhook    SetupWebhook?
}

model userBadges {
  id     String   @id @default(auto()) @map("_id") @db.ObjectId
  badges String[]
  userId String
}

model userData {
  id       String             @id @default(auto()) @map("_id") @db.ObjectId
  user     UserDataUser
  warnings UserDataWarnings[]
}
