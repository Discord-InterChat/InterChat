{
  "name": "discord-chatbot",
  "version": "3.3.2",
  "description": "A growing Discord bot which provides inter-server chat!",
  "main": "build/src/index.js",
  "author": [
    "Supreme1707",
    "dev-737"
  ],
  "license": "GPLv3",
  "scripts": {
    "start": "node .",
    "build": "tsc -p .",
    "dev:watch": "tsc -p . -w",
    "dev:run": "nodemon .",
    "dev": "run-p dev:*",
    "deploy": "tsc && npm start",
    "release": "standard-version",
    "lint": "eslint --cache --fix .",
    "prepare": "husky install"
  },
  "engines": {
    "node": ">=18.2.0"
  },
  "dependencies": {
    "@prisma/client": "^4.6.1",
    "@top-gg/sdk": "^3.1.3",
    "canvacord": "^5.4.8",
    "common-tags": "^1.8.2",
    "discord-xp": "^1.1.16",
    "discord.js": "^14.6.0",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "lodash": "^4.17.21",
    "node-fetch": "^2.6.7",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@types/common-tags": "^1.8.1",
    "@types/express": "^4.17.14",
    "@types/lodash": "^4.14.189",
    "@types/node": "^18.11.9",
    "@types/node-fetch": "^2.6.2",
    "@typescript-eslint/eslint-plugin": "^5.42.1",
    "@typescript-eslint/parser": "^5.42.1",
    "concurrently": "^7.5.0",
    "cz-conventional-changelog": "^3.3.0",
    "esbuild": "^0.15.14",
    "esbuild-node-externals": "^1.5.0",
    "eslint": "^8.27.0",
    "husky": "^8.0.2",
    "lint-staged": "^13.0.3",
    "nodemon": "2.0.20",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "prisma": "^4.6.1",
    "standard-version": "^9.5.0",
    "typescript": "^4.8.4"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "*.ts": "eslint --cache --fix"
  }
}
