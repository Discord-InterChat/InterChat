{
  "name": "interchat",
  "version": "3.24.2",
  "description": "A growing Discord bot which provides inter-server chat!",
  "main": "build/cluster.js",
  "license": "GPL-3.0-or-later",
  "scripts": {
    "start": "node --max-old-space-size=4096 .",
    "start:prod": "pm2 start .ecosystem.config.js",
    "build": "tsc --build",
    "dev": "tsc-watch --outDir ./build --onSuccess \"node --trace-warnings .\"",
    "register:commands": "node build/utils/RegisterCmdCli.js",
    "release": "standard-version",
    "lint": "eslint --cache --fix .",
    "prepare": "husky",
    "postinstall": "husky install"
  },
  "engines": {
    "node": ">=20.11.0"
  },
  "type": "module",
  "dependencies": {
    "@prisma/client": "^5.14.0",
    "@sentry/node": "^8.2.1",
    "@tensorflow/tfjs-node": "^4.19.0",
    "@top-gg/sdk": "^3.1.6",
    "common-tags": "^1.8.2",
    "discord-hybrid-sharding": "^2.2.0",
    "discord.js": "^14.15.2",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "google-translate-api-x": "^10.6.8",
    "husky": "^9.0.11",
    "js-yaml": "^4.1.0",
    "lodash": "^4.17.21",
    "lz-string": "^1.5.0",
    "nsfwjs": "4.1.0",
    "parse-duration": "^1.1.0",
    "source-map-support": "^0.5.21",
    "winston": "^3.13.0"
  },
  "devDependencies": {
    "@stylistic/eslint-plugin": "^2.1.0",
    "@types/common-tags": "^1.8.4",
    "@types/express": "^4.17.21",
    "@types/js-yaml": "^4.0.9",
    "@types/lodash": "^4.17.4",
    "@types/node": "^20.12.12",
    "@types/source-map-support": "^0.5.10",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^9.3.0",
    "lint-staged": "^15.2.2",
    "prettier": "^3.2.5",
    "prisma": "^5.14.0",
    "standard-version": "^9.5.0",
    "tsc-watch": "^6.2.0",
    "typescript": "^5.4.5",
    "typescript-eslint": "^7.10.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "*.ts": "eslint --cache --fix"
  },
  "packageManager": "yarn@4.2.2"
}
