{
  "name": "discord-chatbot",
  "version": "3.5.0",
  "description": "A growing Discord bot which provides inter-server chat!",
  "main": "build/src/index.js",
  "maintainers": [
    {
      "name": "Supreme1707"
    },
    {
      "name": "dev-737"
    }
  ],
  "contributors": [
    {
      "name": "Chanakan55991",
      "url": "https://github.com/Chanakan55991"
    }
  ],
  "license": "AGPL-3.0-or-later",
  "scripts": {
    "start": "node .",
    "tsc": "tsc --build",
    "build": "turbo tsc",
    "dev": "concurrently -k \"tsc -w\" \"nodemon .\"",
    "deploy": "npm run tsc && node build/src/Utils/functions/deploy-commands",
    "release": "standard-version",
    "lint": "eslint --cache --fix .",
    "prepare": "husky install"
  },
  "engines": {
    "node": ">=18.2.0",
    "npm": ">=9.1.2"
  },
  "dependencies": {
    "@prisma/client": "^4.7.1",
    "@top-gg/sdk": "^3.1.3",
    "common-tags": "^1.8.2",
    "discord.js": "^14.7.1",
    "dotenv": "^16.0.3",
    "lodash": "^4.17.21",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@types/common-tags": "^1.8.1",
    "@types/lodash": "^4.14.191",
    "@types/node": "^18.11.11",
    "@typescript-eslint/eslint-plugin": "^5.45.1",
    "@typescript-eslint/parser": "^5.45.1",
    "concurrently": "^7.6.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.29.0",
    "husky": "^8.0.2",
    "lint-staged": "^13.1.0",
    "nodemon": "2.0.20",
    "prettier": "^2.8.1",
    "prisma": "^4.7.1",
    "standard-version": "^9.5.0",
    "turbo": "^1.6.3",
    "typescript": "^4.9.4"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "*.ts": "eslint --cache --fix"
  }
}
