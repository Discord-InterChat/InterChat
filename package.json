{
  "name": "interchat",
  "private": true,
  "version": "4.1.1",
  "description": "A growing Discord bot which provides inter-server chat!",
  "main": "build/index.js",
  "license": "AGPL-3.0-only",
  "scripts": {
    "start": "node --import ./build/instrument.js .",
    "start:prod": "pm2 start .ecosystem.config.js",
    "build": "tsc --build",
    "dev": "tsc-watch --outDir ./build --onSuccess \"node --import ./build/instrument.js .\"",
    "gen:locale-types": "node scripts/genLocaleTypes.js",
    "sync:commands": "node scripts/syncCommands.js",
    "sync:emojis": "node scripts/syncEmojis.js",
    "release": "release-it",
    "lint": "eslint --cache --fix ./src",
    "prepare": "husky"
  },
  "engines": {
    "node": ">=v20.16.0"
  },
  "type": "module",
  "dependencies": {
    "@prisma/client": "^6.1.0",
    "@sentry/node": "^8.47.0",
    "common-tags": "^1.8.2",
    "discord-hybrid-sharding": "^2.2.3",
    "discord.js": "^14.16.3",
    "dotenv": "^16.4.7",
    "express": "^5.0.1",
    "google-translate-api-x": "^10.7.2",
    "husky": "^9.1.7",
    "ioredis": "^5.4.2",
    "js-yaml": "^4.1.0",
    "lodash": "^4.17.21",
    "lz-string": "^1.5.0",
    "ms": "^2.1.3",
    "reflect-metadata": "^0.2.2",
    "source-map-support": "^0.5.21",
    "uuid": "^11.0.3",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@stylistic/eslint-plugin": "^2.12.1",
    "@types/common-tags": "^1.8.4",
    "@types/express": "^5.0.0",
    "@types/js-yaml": "^4.0.9",
    "@types/lodash": "^4.17.13",
    "@types/ms": "^0.7.34",
    "@types/node": "^22.10.2",
    "@types/source-map-support": "^0.5.10",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "9.17.0",
    "lint-staged": "^15.2.11",
    "prettier": "^3.4.2",
    "prisma": "^6.1.0",
    "release-it": "^17.10.0",
    "tsc-watch": "^6.2.1",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.18.1"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "*.ts": "eslint --cache --fix"
  },
  "imports": {
    "#main/*.js": "./build/*.js",
    "#utils/*": "./build/utils/*"
  },
  "packageManager": "pnpm@9.6.0"
}
