{
  "name": "discord-chatbot",
  "version": "3.8.1",
  "description": "A growing Discord bot which provides inter-server chat!",
  "main": "build/src/index.js",
  "maintainers": [
    {
      "name": "Supreme1707"
    },
    {
      "name": "dev-737"
    }
  ],
  "contributors": [
    {
      "name": "Chanakan55991",
      "url": "https://github.com/Chanakan55991"
    }
  ],
  "license": "AGPL-3.0-or-later",
  "scripts": {
    "start": "node .",
    "tsc": "tsc --build",
    "build": "turbo tsc",
    "dev:watch": "tsc --build -w",
    "dev:start": "nodemon .",
    "dev": "npm-run-all build -p dev:*",
    "deploy": "turbo tsc && node build/src/Utils/functions/deploy-commands",
    "release": "standard-version",
    "lint": "eslint --cache --fix .",
    "prepare": "husky install"
  },
  "engines": {
    "node": ">=18.2.0",
    "npm": ">=9.1.2"
  },
  "dependencies": {
    "@prisma/client": "^4.10.1",
    "@sentry/node": "^7.37.2",
    "@top-gg/sdk": "^3.1.3",
    "common-tags": "^1.8.2",
    "discord.js": "^14.8.0-dev.1673006641-0ff67d8.0",
    "dotenv": "^16.0.3",
    "lodash": "^4.17.21",
    "node-schedule": "^2.1.1",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@types/common-tags": "^1.8.1",
    "@types/lodash": "^4.14.191",
    "@types/node": "^18.13.0",
    "@types/node-schedule": "^2.1.0",
    "@typescript-eslint/eslint-plugin": "^5.52.0",
    "@typescript-eslint/parser": "^5.52.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.34.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.1.2",
    "nodemon": "2.0.20",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.4",
    "prisma": "^4.10.1",
    "standard-version": "^9.5.0",
    "turbo": "^1.7.4",
    "typescript": "^4.9.5"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "*.ts": "eslint --cache --fix"
  }
}
